{
  "name": "browser-hex",
  "version": "1.0.0",
  "description": "Pretty-prints a Buffer",
  "main": "hexdump.js",
  "scripts": {
    "test": "tape test/*.js"
  },
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/6",
      "ipad/6",
      "android-browser/latest"
    ]
  },
  "keywords": [
    "hex",
    "print",
    "buffer"
  ],
  "author": "Gabriel Llamas <gagle@outlook.com>",
  "repository": "git://github.com/gagle/node-hex.git",
  "license": "MIT",
  "devDependencies": {
    "tape": "^3.5.0"
  }
}
